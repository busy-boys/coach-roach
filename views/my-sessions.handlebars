            <th scope="col">Superintendent</th>
            <th scope="col">Senior Coordinator</th>
            <th scope="col">Supervisor</th>
          </tr>
        </thead>
        <tbody>
          {{!-- INSTER PENDING HERE --}}
          {{#each pendingSignOff}}
            {{> pending-signoff}}
          {{/each}}
        </tbody>
      </table>
      </div>
      <br><br>
      <div class="container">
        <h3>Past Training</h3>
